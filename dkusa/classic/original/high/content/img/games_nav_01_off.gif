		resetFPS();
	}
	
	// Strategy Guide Overlay
	function show_guide() {
		dropFPS();
		$('#strategy-overlay').show();
		$('#strategy-overlay').load('strategyguide/strategy.html');
	}
	
	function hide_guide() {
		$('#strategy-overlay').hide();
		$('#strategy-overlay').html('');
		resetFPS();
	}

	
	function getFlashMovieObject( movieName ){
		if (window.document[movieName] ){
			return window.document[movieName];
		}
		
		if ( navigator.appName.indexOf( 'Microsoft Internet' ) == -1 ){
			if ( document.embeds && document.embeds[movieName] ) {
				return document.embeds[movieName];
			}
		} else {
			return document.getElementById( movieName );
		}
	}
	
	function resetFPS(){
		var flashObj = getFlashMovieObject('dk');
		flashObj.resetFPS();
	}
	
	function dropFPS(){
		var flashObj = getFlashMovieObject('dk');
		flashObj.dropFPS();
	}

	if (window.location.hash == '#hoardofhype') {
		show_hoard();
	}
	
	if (window.location.hash == '#strategyguide') {
		show_guide()
