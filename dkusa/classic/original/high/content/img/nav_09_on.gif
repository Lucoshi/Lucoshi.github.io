<script type="text/javascript">
function focusPassword(obj,id){
   var elm = document.getElementById(id);
   elm.style.display = "block";
   scroll(0,0);   //for ff
   obj.style.display = "none";
   elm.focus();
   scroll(0,0);   //for ff
}
function blurPassword(obj,id){
   var elm = document.getElementById(id);
   if( obj.value == '' ){
      elm.style.display = "block";
      obj.style.display = "none";
   }
}
</script>

